export m; m=$1
shift
rm -f ${m}.d_f ${m}.d_n ${m}.d_o ${m}.d_v
touch ${m}.d_f ${m}.d_n ${m}.d_o ${m}.d_v
echo '# This file was automatically generated by Ghostscript (gssetmod).' >${m}.dev
echo cat ${m}.d_n '>>_temp_.dh' >>${m}.dev
echo cat ${m}.d_o '>>_temp_.dob' >>${m}.dev
echo cat ${m}.d_v '>>_temp_.dop' >>${m}.dev
./gsaddmod ${m} -obj $*
